<!DOCTYPE html>
<html>
  <head>
    <title>Hot Potatoes</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <head>
    <nav class="navbar navbar-toggleable-md navbar-light bg-faded mb-3">
      <div class="container">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" href="#">Navbar</a>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item active">
              <%= link_to "Quizzes", v2_quizzes_path, class: "nav-link" %>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
